@page
@namespace AzerothMemories.WebServer.Pages
@using AzerothMemories.WebBlazor
@using Microsoft.AspNetCore.Mvc.TagHelpers
@using Stl.Fusion.Server.Authentication
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@inject ServerAuthHelper _serverAuthHelper

@{
    Layout = "_Layout";

    var sessionId = _serverAuthHelper.Session.Id.Value;
}

<component type="typeof(App)" render-mode="WebAssemblyPrerendered" param-SessionId="@sessionId" />